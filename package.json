{
  "name": "reportesobm",
  "version": "1.0.0",
  "main": "frontend/js/main.js",
  "private": true,
  "scripts": {
    "predev": "npm run build:css",
    "dev": "vite",
    "prebuild": "npm run build:css",
    "build": "vite build",
    "build:css": "tailwindcss -i frontend/css/styles.css -o frontend/css/styles.build.css",
    "preview": "vite preview",
    "lint": "eslint \"frontend/js/**/*.js\"",
    "format": "prettier --write \"frontend/js/**/*.{js,json,css,html}\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "release:verify": "npm run lint && npm test && npm run build",
    "predeploy": "npm run release:verify",
    "clasp:push": "npx @google/clasp push --force",
    "clasp:deploy": "npx @google/clasp deploy --description \"Auto deploy\"",
    "clasp:check": "npx @google/clasp --version",
    "clasp:test": "npx @google/clasp run testCrearRemito --json",
    "prepare": "husky install",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@jest/environment-jsdom": "npm:jest-environment-jsdom@^30.1.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "fetch-mock": "^12.5.4",
    "gh-pages": "^6.3.0",
    "jest": "^30.1.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0",
    "husky": "^8.0.3"
  }
}
